        <parameters>
          <parameter name="value" type="string">
            <brief>
              <para xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">String to be cleaned.</para>
            </brief>
          </parameter>
          <parameter name="removeCarriageReturn" type="bool">
